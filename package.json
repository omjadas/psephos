{
  "name": "psephos",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/omjadas/electionSimulator.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "npm run build:schema",
    "build:all": "npm run build && cd client && npm run build",
    "build:client": "cd client && npm run build",
    "build:schema": "ts-node-script ./scripts/schema.ts",
    "ci:all": "npm ci && cd client && npm ci",
    "ci:client": "cd client && npm ci",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.ts\" \"test/**/*.ts\"",
    "prune:all": "npm prune && cd client && npm prune",
    "prune:client": "cd client && npm prune",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --config jest.config.json",
    "test:watch": "jest --coverage --watch --config jest.config.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.json",
    "test:e2e": "jest --config ./test/jest.e2e.config.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjsplus/cookies": "^1.1.1",
    "@sendgrid/mail": "^7.4.4",
    "apollo-server-express": "^2.25.0",
    "bcrypt": "^5.0.1",
    "caritat": "0.0.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "graphql": "^15.5.0",
    "graphql-tag-pluck": "^0.8.7",
    "graphql-tools": "^7.0.5",
    "helmet": "^4.6.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "slugify": "^1.5.3",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.17",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.11.1",
    "@types/express": "^4.17.12",
    "@types/graphql": "^14.5.0",
    "@types/hapi__joi": "^17.1.6",
    "@types/helmet": "4.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.17.1",
    "@types/passport-google-oauth20": "^2.0.7",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  }
}
