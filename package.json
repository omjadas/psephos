{
  "name": "psephos",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/omjadas/electionSimulator.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "npm run build:schema",
    "build:all": "npm run build && cd client && npm run build",
    "build:client": "cd client && npm run build",
    "build:schema": "ts-node-script ./scripts/schema.ts",
    "ci:all": "npm ci && cd client && npm ci",
    "ci:client": "cd client && npm ci",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.ts\" \"test/**/*.ts\"",
    "prune:all": "npm prune && cd client && npm prune",
    "prune:client": "cd client && npm prune",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --config jest.config.json",
    "test:watch": "jest --coverage --watch --config jest.config.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.json",
    "test:e2e": "jest --config ./test/jest.e2e.config.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.6.5",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.5",
    "@nestjs/graphql": "^7.9.4",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.5",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjsplus/cookies": "^1.1.1",
    "@sendgrid/mail": "^7.4.0",
    "apollo-server-express": "^2.19.1",
    "bcrypt": "^5.0.0",
    "caritat": "0.0.4",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "graphql": "^15.4.0",
    "graphql-tag-pluck": "^0.8.7",
    "graphql-tools": "^7.0.2",
    "helmet": "^4.3.1",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "slugify": "^1.4.6",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.6",
    "@nestjs/testing": "^7.6.5",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.11.0",
    "@types/express": "^4.17.9",
    "@types/graphql": "^14.5.0",
    "@types/hapi__joi": "^17.1.6",
    "@types/helmet": "4.0.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.17",
    "@types/passport-google-oauth20": "^2.0.6",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.13",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
